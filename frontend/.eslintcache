escription}\r\n                  </ListGroup.Item>\r\n                </ListGroup>\r\n              </Col>\r\n\r\n              <Col md={3}>\r\n                <Card>\r\n                  <ListGroup variant='flush'>\r\n                    <ListGroup.Item>\r\n                      <Row>\r\n                        <Col> Price: </Col>\r\n                        <Col><strong> {product.price} </strong></Col>\r\n                      </Row>\r\n                    </ListGroup.Item>\r\n\r\n                    <ListGroup.Item>\r\n                      <Row>\r\n                        <Col> Status: </Col>\r\n                        <Col>\r\n                          {product.countInStock > 0 ? 'In Stock' : 'Out of Stock'}\r\n                        </Col>\r\n                      </Row>\r\n                    </ListGroup.Item>\r\n\r\n                    {\r\n                      product.countInStock > 0 && (\r\n                        <ListGroup.Item>\r\n                          <Row>\r\n                            <Col>Quantity</Col>\r\n                            <Col>\r\n                              <Form.Control\r\n                                as='select'\r\n                                value={qty}\r\n                                onChange={(e) => setQty(e.target.value)}\r\n                              >{\r\n                                  [...Array(product.countInStock).keys()].map(x => (\r\n                                    <option key={x + 1} value={x + 1}>{x + 1}</option>\r\n                                  ))\r\n                                }\r\n                              </Form.Control>\r\n                            </Col>\r\n                          </Row>\r\n                        </ListGroup.Item>\r\n                      )\r\n                    }\r\n\r\n                    <ListGroup.Item>\r\n                      <Button\r\n                        onClick={addToCartHandler}\r\n                        className='btn-block'\r\n                        type='button'\r\n                        disabled={product.countInStock === 0}\r\n                      >Add To Cart</Button>\r\n                    </ListGroup.Item>\r\n                  </ListGroup>\r\n                </Card>\r\n              </Col>\r\n            </Row>)\r\n      }\r\n\r\n    </>\r\n  )\r\n}\r\n\r\nexport default ProductScreen\r\n","E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\store.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\reducers\\productReducers.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\constants\\productConstants.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\actions\\productActions.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\components\\Loader.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\components\\Message.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\screens\\CartScreen.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\reducers\\cartReducers.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\constants\\cartConstants.js",[],"E:\\Murtaza\\Projects\\Proshop\\frontend\\src\\actions\\cartActions.js",[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":17,"column":11,"nodeType":"89","messageId":"90","endLine":17,"endColumn":20},"no-native-reassign",["91"],"no-negated-in-lhs",["92"],"no-unused-vars","'cartItems' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]